<!DOCTYPE html>
<html lang="de">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>AEra Login</title>
    <style>
        * { margin: 0; padding: 0; box-sizing: border-box; }
        body {
            font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;
            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            min-height: 100vh;
            display: flex;
            justify-content: center;
            align-items: center;
            padding: 20px;
        }
        .container {
            background: white;
            border-radius: 12px;
            box-shadow: 0 10px 40px rgba(0,0,0,0.3);
            padding: 40px;
            max-width: 500px;
            width: 100%;
        }
        .header {
            text-align: center;
            margin-bottom: 30px;
        }
        .logo { font-size: 48px; margin-bottom: 10px; }
        .title { font-size: 24px; font-weight: bold; color: #667eea; margin-bottom: 5px; }
        .tagline { font-size: 12px; color: #999; }
        .button {
            width: 100%;
            padding: 14px;
            border: none;
            border-radius: 8px;
            font-size: 16px;
            font-weight: 600;
            cursor: pointer;
            margin-bottom: 12px;
            transition: all 0.3s ease;
            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            color: white;
        }
        .button:hover { transform: translateY(-2px); box-shadow: 0 8px 20px rgba(102,126,234,0.4); }
        .button:disabled { 
            opacity: 0.5; 
            cursor: not-allowed;
            transform: none;
            box-shadow: none;
        }
        .status {
            padding: 16px;
            border-radius: 8px;
            margin-bottom: 16px;
            display: none;
            font-size: 14px;
        }
        .status.show { display: block; }
        .status.success { background: #d4edda; color: #155724; border: 1px solid #c3e6cb; }
        .status.error { background: #f8d7da; color: #721c24; border: 1px solid #f5c6cb; }
        .status.info { background: #d1ecf1; color: #0c5460; border: 1px solid #bee5eb; }
        .info-box {
            background: #f0f7ff;
            border-left: 4px solid #667eea;
            padding: 14px;
            border-radius: 6px;
            font-size: 13px;
            color: #333;
            margin-bottom: 20px;
        }
        .details {
            background: #f8f9fa;
            border-radius: 8px;
            padding: 16px;
            margin-top: 20px;
            display: none;
        }
        .details.show { display: block; }
        .detail-row {
            display: flex;
            justify-content: space-between;
            padding: 10px 0;
            border-bottom: 1px solid #e0e0e0;
            font-size: 13px;
        }
        .detail-row:last-child { border-bottom: none; }
        .detail-label { color: #666; font-weight: 600; }
        .detail-value { color: #333; text-align: right; flex: 1; margin-left: 10px; word-break: break-all; }
        .score-badge {
            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            color: white;
            padding: 6px 14px;
            border-radius: 20px;
            font-weight: bold;
            font-size: 13px;
        }
        .footer {
            text-align: center;
            font-size: 11px;
            color: #999;
            margin-top: 20px;
            padding-top: 16px;
            border-top: 1px solid #eee;
        }
        .debug-info {
            background: #fafafa;
            border: 1px solid #e0e0e0;
            padding: 10px;
            border-radius: 6px;
            font-size: 11px;
            color: #666;
            font-family: monospace;
            margin-top: 16px;
            max-height: 150px;
            overflow-y: auto;
            display: none;
        }
        .debug-info.show { display: block; }
    </style>
</head>
<body>
    <div class="container">
        <div class="header">
            <div class="logo">‚ö°</div>
            <div class="title">AEra Login</div>
            <div class="tagline">Proof of Human via Resonance</div>
        </div>
        
        <div class="info-box">
            üí° Verbinde deine Wallet ‚Äì ohne KYC, ohne Klardaten, ohne Datenverlust
        </div>
        
        <div id="status" class="status"></div>
        
        <div id="desktopView">
            <button id="connectBtn" class="button">üîó Wallet Verbinden</button>
            <button id="verifyBtn" class="button" style="display: none;">‚úì Verifizieren</button>
            <button id="disconnectBtn" class="button" style="display: none; background: #999;">‚úï Trennen</button>
        </div>
        
        <div id="details" class="details">
            <div class="detail-row">
                <span class="detail-label">Status:</span>
                <span class="detail-value" id="statusValue">‚Äî</span>
            </div>
            <div class="detail-row">
                <span class="detail-label">Wallet:</span>
                <span class="detail-value" id="addressValue">‚Äî</span>
            </div>
            <div class="detail-row">
                <span class="detail-label">Score:</span>
                <span class="detail-value" id="scoreValue">‚Äî</span>
            </div>
            <div class="detail-row">
                <span class="detail-label">Logins:</span>
                <span class="detail-value" id="loginCountValue">‚Äî</span>
            </div>
        </div>
        
        <div id="debugInfo" class="debug-info"></div>
        
        <div class="footer">AEra Login ¬© 2025 | CC BY-NC-SA 4.0</div>
    </div>
    
    <script>
        const API_BASE = window.location.origin;
        
        let connectBtn, verifyBtn, disconnectBtn, statusDiv, detailsDiv, debugInfo;
        let currentAddress = null;
        let currentToken = null;
        
        function log(msg) {
            console.log('[AEra]', msg);
            debugInfo.classList.add('show');
            debugInfo.innerHTML = msg + '<br>' + debugInfo.innerHTML;
            if (debugInfo.children.length > 50) {
                debugInfo.innerHTML = debugInfo.innerHTML.substring(0, debugInfo.innerHTML.lastIndexOf('<br>', debugInfo.innerHTML.length - 100));
            }
        }
        
        function showStatus(message, type = 'info') {
            statusDiv.textContent = message;
            statusDiv.className = `status ${type} show`;
            log(`[${type}] ${message}`);
        }
        
        async function autoLoginWithToken() {
            const savedToken = localStorage.getItem('aera_token');
            const savedAddress = localStorage.getItem('aera_address');
            if (!savedToken || !savedAddress) {
                log('‚ÑπÔ∏è Kein gespeichertes Token gefunden');
                return false;
            }
            
            try {
                log('üîê Versuche Auto-Login mit echtem MetaMask-Check...');
                
                // ===== STRIKTE BLOCKADE: ECHTE METAMASK PR√úFUNG =====
                if (typeof window.ethereum === 'undefined') {
                    log('‚ùå‚ùå‚ùå MetaMask NICHT INSTALLIERT - Auto-Login BLOCKIERT');
                    localStorage.removeItem('aera_token');
                    localStorage.removeItem('aera_address');
                    showStatus('‚ùå MetaMask nicht installiert! Auto-Login BLOCKIERT', 'error');
                    return false;
                }
                
                // ECHTE Pr√ºfung mit eth_requestAccounts (nicht eth_accounts!)
                let accounts = [];
                try {
                    log('‚è≥ Fordere eth_requestAccounts an...');
                    accounts = await window.ethereum.request({ method: 'eth_requestAccounts' });
                } catch (e) {
                    log('‚ùå‚ùå‚ùå eth_requestAccounts fehlgeschlagen: ' + e.message);
                    log('    ‚Üí MetaMask ist nicht aktiv oder wurde abgelehnt');
                    localStorage.removeItem('aera_token');
                    localStorage.removeItem('aera_address');
                    showStatus('‚ùå MetaMask nicht aktiv! Auto-Login BLOCKIERT', 'error');
                    return false;
                }
                
                if (!accounts || accounts.length === 0) {
                    log('‚ùå‚ùå‚ùå Keine Wallet-Konten verf√ºgbar - Auto-Login BLOCKIERT');
                    localStorage.removeItem('aera_token');
                    localStorage.removeItem('aera_address');
                    showStatus('‚ùå Keine Wallet verbunden! Auto-Login BLOCKIERT', 'error');
                    return false;
                }
                
                const currentWallet = accounts[0].toLowerCase();
                const savedWallet = savedAddress.toLowerCase();
                
                if (currentWallet !== savedWallet) {
                    log('‚ùå‚ùå‚ùå WALLET MISMATCH - Auto-Login BLOCKIERT (SafeWallet-Modus)');
                    log(`   Registriert: ${savedWallet}`);
                    log(`   Aktuell: ${currentWallet}`);
                    localStorage.removeItem('aera_token');
                    localStorage.removeItem('aera_address');
                    showStatus('‚ùå Falsche Wallet! Auto-Login BLOCKIERT', 'error');
                    return false;
                }
                
                log('‚úì MetaMask-Sicherheitspr√ºfung bestanden!');
                
                // ===== NEUE SIGNATUR-VERIFIKATION AUCH F√úR AUTO-LOGIN =====
                log('üìã Schritt 1: Nonce vom Server anfordern...');
                
                const nonceResponse = await fetch(`${API_BASE}/api/nonce`, {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({ address: currentWallet }),
                    signal: AbortSignal.timeout(5000)
                });
                
                if (!nonceResponse.ok) throw new Error(`Nonce Request HTTP ${nonceResponse.status}`);
                
                const nonceData = await nonceResponse.json();
                if (!nonceData.success) throw new Error('Nonce generation failed');
                
                const messageToSign = nonceData.message;
                log('‚úì Nonce empfangen: ' + nonceData.nonce);
                
                // ===== SCHRITT 2: METAMASK SIGNATUR ANFORDERN =====
                log('üîê Schritt 2: Fordere MetaMask zum Signieren auf...');
                
                let signature;
                try {
                    signature = await window.ethereum.request({
                        method: 'personal_sign',
                        params: [messageToSign, currentWallet]
                    });
                } catch (signError) {
                    log('‚ùå‚ùå‚ùå MetaMask Signierung abgelehnt: ' + signError.message);
                    localStorage.removeItem('aera_token');
                    localStorage.removeItem('aera_address');
                    showStatus('‚ùå MetaMask Signierung abgelehnt! Auto-Login BLOCKIERT', 'error');
                    return false;
                }
                
                if (!signature) {
                    log('‚ùå‚ùå‚ùå Keine Signatur von MetaMask - Auto-Login BLOCKIERT');
                    localStorage.removeItem('aera_token');
                    localStorage.removeItem('aera_address');
                    showStatus('‚ùå Keine Signatur erhalten! Auto-Login BLOCKIERT', 'error');
                    return false;
                }
                
                log('‚úì Signatur erhalten');
                
                // ===== SCHRITT 3: TOKEN + SIGNATUR AN SERVER SENDEN =====
                log('‚úì Schritt 3: Verifiziere auf dem Server...');
                
                const response = await fetch(`${API_BASE}/api/verify-token`, {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({
                        token: savedToken,
                        address: currentWallet,
                        nonce: nonceData.nonce,
                        message: messageToSign,
                        signature: signature
                    }),
                    signal: AbortSignal.timeout(5000)
                });
                
                const data = await response.json();
                
                if (!data.valid && data.error) {
                    log('‚ö†Ô∏è Token-Verifikation: ' + data.error);
                    localStorage.removeItem('aera_token');
                    localStorage.removeItem('aera_address');
                    return false;
                }
                
                if (!data.valid) {
                    log('‚ö†Ô∏è Token ung√ºltig oder abgelaufen');
                    localStorage.removeItem('aera_token');
                    localStorage.removeItem('aera_address');
                    return false;
                }
                
                currentAddress = data.address;
                currentToken = savedToken;
                
                log('‚úì‚úì‚úì Auto-Login ERFOLGREICH mit Signatur-Verifikation!');
                showStatus(`‚úì Auto-Login erfolgreich`, 'success');
                
                document.getElementById('addressValue').textContent = currentAddress;
                document.getElementById('statusValue').textContent = '‚úÖ Auto-Login';
                document.getElementById('scoreValue').innerHTML = 
                    `<span class="score-badge">${data.resonance_score}/100</span>`;
                document.getElementById('loginCountValue').textContent = data.login_count;
                
                connectBtn.style.display = 'none';
                verifyBtn.style.display = 'none';
                disconnectBtn.style.display = 'block';
                detailsDiv.classList.add('show');
                
                return true;
                
            } catch (error) {
                log('‚ùå Auto-Login: ' + error.message);
                localStorage.removeItem('aera_token');
                localStorage.removeItem('aera_address');
                return false;
            }
        }
        
        async function connectWallet() {
            try {
                log('‚è≥ Wallet-Verbindung startet...');
                
                // ===== CHECK 1: MetaMask installiert? =====
                if (typeof window.ethereum === 'undefined') {
                    log('‚ùå‚ùå‚ùå MetaMask NICHT INSTALLIERT');
                    showStatus('‚ùå MetaMask nicht installiert! Login BLOCKIERT', 'error');
                    return;
                }
                
                connectBtn.disabled = true;
                
                // ===== CHECK 2: Ist MetaMask WIRKLICH offen/aktiv? =====
                log('üîç √úberpr√ºfe ob MetaMask WIRKLICH aktiv ist...');
                
                // Versuche zuerst eth_accounts (schnell, cached)
                let cachedAccounts = [];
                try {
                    cachedAccounts = await window.ethereum.request({ method: 'eth_accounts' });
                    log('‚ÑπÔ∏è  Cached Accounts gefunden: ' + (cachedAccounts.length > 0 ? '‚úì' : '‚ùå'));
                } catch (e) {
                    log('‚ö†Ô∏è eth_accounts fehlgeschlagen (OK, ist optional)');
                }
                
                // Jetzt ERZWINGE echte Verbindung mit eth_requestAccounts
                // Das Popup wird SICHTBAR gezeigt wenn MetaMask offen ist
                log('‚è≥ Warte auf MetaMask-Popup (muss SICHTBAR erscheinen!)...');
                showStatus('‚è≥ MetaMask-Popup sollte oben rechts sichtbar sein!', 'info');
                
                let accounts = [];
                try {
                    // WICHTIG: timeout=5000 bedeutet "wenn keine Antwort in 5s, dann Error"
                    // Das verhindert dass gecachte Werte akzeptiert werden
                    const requestPromise = window.ethereum.request({ 
                        method: 'eth_requestAccounts'
                    });
                    
                    // Set timeout um sicherzustellen dass MetaMask wirklich antwortet
                    const timeoutPromise = new Promise((_, reject) => 
                        setTimeout(() => reject(new Error('MetaMask antwortete nicht - ist es offen?')), 8000)
                    );
                    
                    accounts = await Promise.race([requestPromise, timeoutPromise]);
                    
                } catch (popupError) {
                    log('‚ùå‚ùå‚ùå MetaMask Popup abgelehnt oder nicht verf√ºgbar:');
                    log('    Error: ' + popupError.message);
                    log('    ‚Üí MetaMask MUSS offen/aktiv sein!');
                    showStatus('‚ùå MetaMask nicht verf√ºgbar! Bitte MetaMask √∂ffnen und versuchen Sie es erneut.', 'error');
                    connectBtn.disabled = false;
                    return;
                }
                
                if (!accounts || accounts.length === 0) {
                    log('‚ùå‚ùå‚ùå Keine Wallet-Konten nach eth_requestAccounts');
                    log('    ‚Üí Keine Wallet in MetaMask verbunden!');
                    showStatus('‚ùå Keine Wallet in MetaMask gefunden! Login BLOCKIERT', 'error');
                    connectBtn.disabled = false;
                    return;
                }
                
                currentAddress = accounts[0];
                log('‚úì‚úì‚úì MetaMask ECHTE Verbindung best√§tigt mit: ' + currentAddress);
                log('    (Popup wurde angezeigt und User hat best√§tigt)');
                
                showStatus(`‚úì Wallet verbunden (MetaMask AKTIV). Jetzt "Verifizieren" klicken!`, 'success');
                
                document.getElementById('addressValue').textContent = currentAddress;
                document.getElementById('statusValue').textContent = 'üîó Verbunden';
                
                connectBtn.style.display = 'none';
                verifyBtn.style.display = 'block';
                verifyBtn.disabled = false;
                disconnectBtn.style.display = 'block';
                detailsDiv.classList.add('show');
                
                log('‚úÖ Button "Verifizieren" ist jetzt aktiv!');
                
            } catch (error) {
                log('‚ùå connectWallet Exception: ' + error.message);
                showStatus('‚ùå Fehler: ' + error.message, 'error');
                connectBtn.disabled = false;
            }
        }
        
        async function verifyWallet() {
            if (!currentAddress) {
                log('‚ùå Keine Wallet verbunden');
                return;
            }
            
            try {
                // ===== STRIKTE METAMASK CHECK AUCH BEI VERIFY - NUTZE eth_requestAccounts =====
                if (typeof window.ethereum === 'undefined') {
                    log('‚ùå‚ùå‚ùå MetaMask wurde entfernt/deinstalliert - VERIFY BLOCKIERT');
                    showStatus('‚ùå MetaMask nicht verf√ºgbar! VERIFY BLOCKIERT', 'error');
                    return;
                }
                
                log('üîê SICHERHEITSPR√úFUNG: Verifiziere dass MetaMask NOCH aktiv ist...');
                
                // Pr√ºfe JETZT ob MetaMask ECHTE noch aktiv ist
                let accounts = [];
                try {
                    accounts = await window.ethereum.request({ method: 'eth_requestAccounts' });
                } catch (e) {
                    log('‚ùå‚ùå‚ùå eth_requestAccounts fehlgeschlagen: ' + e.message);
                    showStatus('‚ùå MetaMask nicht verf√ºgbar! VERIFY BLOCKIERT', 'error');
                    return;
                }
                
                if (!accounts || accounts.length === 0) {
                    log('‚ùå‚ùå‚ùå Keine Wallet nach eth_requestAccounts - VERIFY BLOCKIERT');
                    showStatus('‚ùå MetaMask nicht aktiv! VERIFY BLOCKIERT', 'error');
                    return;
                }
                
                // Pr√ºfe ob es NOCH die gleiche Wallet ist
                if (accounts[0].toLowerCase() !== currentAddress.toLowerCase()) {
                    log('‚ùå‚ùå‚ùå WALLET GEWECHSELT - VERIFY BLOCKIERT');
                    log(`   Urspr√ºngliche: ${currentAddress}`);
                    log(`   Aktuelle: ${accounts[0]}`);
                    showStatus('‚ùå Wallet wurde gewechselt! VERIFY BLOCKIERT', 'error');
                    return;
                }
                
                log('‚úì‚úì‚úì MetaMask Sicherheitspr√ºfung (eth_requestAccounts) bestanden!');
                verifyBtn.disabled = true;
                showStatus('‚è≥ Verifizierung l√§uft... (bitte auf MetaMask Popup warten)', 'info');
                
                // ===== NEUE SIGNATUR-VERIFIKATION (KRYPTOGRAPHISCH SICHER) =====
                log('üìã Schritt 1: Nonce vom Server anfordern...');
                
                const nonceResponse = await fetch(`${API_BASE}/api/nonce`, {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({ address: currentAddress }),
                    signal: AbortSignal.timeout(5000)
                });
                
                if (!nonceResponse.ok) throw new Error(`Nonce Request HTTP ${nonceResponse.status}`);
                
                const nonceData = await nonceResponse.json();
                if (!nonceData.success) throw new Error('Nonce generation failed');
                
                const messageToSign = nonceData.message;
                log('‚úì Nonce empfangen: ' + nonceData.nonce);
                log('üìù Nachricht zum Signieren (du wirst das in MetaMask sehen):');
                log(messageToSign);
                
                // ===== SCHRITT 2: METAMASK SIGNATUR ANFORDERN =====
                log('üîê Schritt 2: Fordere MetaMask zum Signieren auf...');
                
                let signature;
                try {
                    signature = await window.ethereum.request({
                        method: 'personal_sign',
                        params: [messageToSign, currentAddress]
                    });
                } catch (signError) {
                    log('‚ùå‚ùå‚ùå MetaMask Signierung abgelehnt oder fehlgeschlagen: ' + signError.message);
                    showStatus('‚ùå MetaMask Signierung abgelehnt! VERIFY BLOCKIERT', 'error');
                    verifyBtn.disabled = false;
                    return;
                }
                
                if (!signature) {
                    log('‚ùå‚ùå‚ùå Keine Signatur von MetaMask - VERIFY BLOCKIERT');
                    showStatus('‚ùå Keine Signatur erhalten! VERIFY BLOCKIERT', 'error');
                    verifyBtn.disabled = false;
                    return;
                }
                
                log('‚úì Signatur erhalten: ' + signature.substring(0, 20) + '...');
                
                // ===== SCHRITT 3: SIGNATUR AN SERVER SENDEN =====
                log('‚úì Schritt 3: Verifiziere Signatur auf dem Server...');
                
                const response = await fetch(`${API_BASE}/api/verify`, {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({
                        address: currentAddress,
                        nonce: nonceData.nonce,
                        message: messageToSign,
                        signature: signature
                    }),
                    signal: AbortSignal.timeout(5000)
                });
                
                if (!response.ok) throw new Error(`HTTP ${response.status}`);
                
                const data = await response.json();
                
                if (data.error) throw new Error(data.error);
                if (!data.is_human) throw new Error('Signaturverifikation fehlgeschlagen');
                
                log('‚úì‚úì‚úì Signatur VERIFIZIERT - Login AUTORISIERT!');
                
                document.getElementById('scoreValue').innerHTML = 
                    `<span class="score-badge">${data.resonance_score}/100</span>`;
                document.getElementById('statusValue').textContent = '‚úÖ Verifiziert';
                document.getElementById('loginCountValue').textContent = data.login_count;
                
                showStatus(`‚úì Score: ${data.resonance_score}/100 - Login erfolgreich!`, 'success');
                verifyBtn.textContent = '‚úì Verifiziert';
                
                if (data.token) {
                    currentToken = data.token;
                    localStorage.setItem('aera_token', data.token);
                    localStorage.setItem('aera_address', currentAddress);
                    log('üíæ Token gespeichert (k√∂nnen sp√§ter Auto-Login machen)');
                }
                
                if (data.message && data.message.includes('Airdrop')) {
                    showStatus(data.message, 'success');
                    log('üéÅ ' + data.message);
                }
                
            } catch (error) {
                log('‚ùå Fehler: ' + error.message);
                showStatus('Fehler: ' + error.message, 'error');
                verifyBtn.textContent = '‚úì Verifizieren';  // Reset button text
            } finally {
                verifyBtn.disabled = false;
            }
        }
        
        function disconnectWallet() {
            currentAddress = null;
            currentToken = null;
            
            localStorage.removeItem('aera_token');
            localStorage.removeItem('aera_address');
            
            document.getElementById('addressValue').textContent = '‚Äî';
            document.getElementById('statusValue').textContent = '‚Äî';
            document.getElementById('scoreValue').textContent = '‚Äî';
            document.getElementById('loginCountValue').textContent = '‚Äî';
            
            connectBtn.style.display = 'block';
            verifyBtn.style.display = 'none';
            disconnectBtn.style.display = 'none';
            verifyBtn.textContent = '‚úì Verifizieren';
            verifyBtn.disabled = false;
            detailsDiv.classList.remove('show');
            
            showStatus('‚úì Wallet getrennt', 'info');
            log('üóëÔ∏è Session gel√∂scht');
        }
        
        document.addEventListener('DOMContentLoaded', function() {
            try {
                connectBtn = document.getElementById('connectBtn');
                verifyBtn = document.getElementById('verifyBtn');
                disconnectBtn = document.getElementById('disconnectBtn');
                statusDiv = document.getElementById('status');
                detailsDiv = document.getElementById('details');
                debugInfo = document.getElementById('debugInfo');
                
                log('=== AEra Login Minimal gestartet ===');
                log('API: ' + API_BASE);
                
                connectBtn.addEventListener('click', connectWallet);
                verifyBtn.addEventListener('click', verifyWallet);
                disconnectBtn.addEventListener('click', disconnectWallet);
                
                // ===== VERSUCHE AUTO-LOGIN (ABER NICHT BLOCKIEREND) =====
                // Die echte Blockade kommt durch eth_requestAccounts in connectWallet() und verifyWallet()
                (async () => {
                    if (typeof window.ethereum !== 'undefined') {
                        log('MetaMask installiert - versuche Auto-Login...');
                        
                        setTimeout(async () => {
                            try {
                                await autoLoginWithToken();
                            } catch (error) {
                                log('‚ùå Auto-Login Exception: ' + error.message);
                            }
                        }, 300);
                    } else {
                        log('‚ö†Ô∏è MetaMask nicht installiert');
                    }
                })();
            } catch (error) {
                console.error('INIT ERROR:', error);
                log('‚ùå INIT FEHLER: ' + error.message);
            }
        });
    </script>
</body>
</html>
